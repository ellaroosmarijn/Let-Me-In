{
  "name": "letmein",
  "version": "1.0.0",
  "description": "Zoom waiting room game.",
  "main": "server/api/index.ts",
  "license": "MIT",
  "author": "tohora2023",
  "keywords": [
    "let",
    "me",
    "in"
  ],
  "scripts": {
    "start": "ts-node server/api/index.ts",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon --watch server server/api/index.ts",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "webpack": "webpack --config ./client/webpack.config.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --noStackTrace --watchAll"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.0.1",
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.6",
    "@mantine/form": "^6.0.6",
    "@mantine/hooks": "^6.0.6",
    "@mantine/notifications": "^6.0.6",
    "@redux-devtools/extension": "^3.2.3",
    "aws-sdk": "^2.1350.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "express-jwt-authz": "^2.4.1",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^2.1.5",
    "knex": "^2.3.0",
    "pg": "^8.8.0",
    "sqlite3": "^5.1.2",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@devacademy/eslint-config": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4",
    "@testing-library/user-event": "^12.8.3",
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/supertest": "^2.0.12",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.2",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.3.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.4.2",
    "sass": "^1.50.1",
    "sass-loader": "12.6.0",
    "style-loader": "^3.3.2",
    "supertest": "^6.2.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
